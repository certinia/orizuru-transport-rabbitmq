{
	"name": "@financialforcedev/orizuru-transport-rabbitmq",
	"version": "4.0.0",
	"description": "Rabbitmq transport layer for the orizuru package",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"doc": "typedoc --ignoreCompilerErrors --out doc --mode file",
		"init-project": "cp run.properties local.run.properties && cp test.properties local.test.properties",
		"prepare": "npm run build",
		"pretest": "tslint -p tsconfig.json",
		"test": "nyc --nycrc-path=test/nyc.opts mocha"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/financialforcedev/orizuru-transport-rabbitmq"
	},
	"keywords": [
		"rabbitmq",
		"publish",
		"subscribe",
		"heroku",
		"worker",
		"dyno",
		"orizuru"
	],
	"author": "financialforce",
	"license": "BSD-3-Clause",
	"dependencies": {
		"@types/amqplib": "0.5.8",
		"@types/lodash": "^4.14.116",
		"@types/node": "^10.7.1",
		"amqplib": "^0.5.2",
		"lodash": "^4.17.10"
	},
	"devDependencies": {
		"@financialforcedev/orizuru": "^7.0.0",
		"@types/chai": "^4.1.4",
		"@types/chai-as-promised": "^7.1.0",
		"@types/mocha": "^5.2.5",
		"@types/sinon": "^5.0.1",
		"@types/sinon-chai": "^3.2.0",
		"chai": "^4.1.2",
		"chai-as-promised": "^7.1.1",
		"mocha": "^5.2.0",
		"nyc": "^12.0.2",
		"sinon": "^6.1.5",
		"sinon-chai": "^3.2.0",
		"ts-node": "^7.0.1",
		"tslint": "^5.11.0",
		"typedoc": "^0.12.0",
		"typescript": "^3.0.1"
	}
}
