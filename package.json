{
	"name": "@financialforcedev/orizuru-transport-rabbitmq",
	"version": "4.0.0",
	"description": "Rabbitmq transport layer for the orizuru package",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"doc": "typedoc --ignoreCompilerErrors --out doc --mode file",
		"init-project": "cp run.properties local.run.properties && cp test.properties local.test.properties",
		"prepare": "npm run build",
		"pretest": "tslint -p tsconfig.json",
		"test": "nyc mocha --require ts-node/register --watch-extensions ts test/*.test.ts test/**/*.test.ts test/**/**/*.test.ts"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/financialforcedev/orizuru-transport-rabbitmq"
	},
	"keywords": [
		"rabbitmq",
		"publish",
		"subscribe",
		"heroku",
		"worker",
		"dyno",
		"orizuru"
	],
	"author": "financialforce",
	"license": "BSD-3-Clause",
	"dependencies": {
		"@types/amqplib": "0.5.7",
		"@types/lodash": "4.14.110",
		"@types/node": "10.3.4",
		"amqplib": "0.5.2",
		"lodash": "4.17.10"
	},
	"devDependencies": {
		"@financialforcedev/orizuru": "git://github.com/financialforcedev/orizuru.git#feature/adopt-typescript",
		"@types/chai": "4.1.4",
		"@types/chai-as-promised": "7.1.0",
		"@types/mocha": "5.2.3",
		"@types/sinon": "5.0.1",
		"@types/sinon-chai": "3.2.0",
		"chai": "4.1.2",
		"chai-as-promised": "7.1.1",
		"mocha": "5.2.0",
		"nyc": "12.0.2",
		"sinon": "6.0.0",
		"sinon-chai": "3.2.0",
		"ts-node": "6.1.2",
		"tslint": "5.10.0",
		"typedoc": "0.11.1",
		"typescript": "2.9.2"
	},
	"nyc": {
		"check-coverage": true,
		"per-file": true,
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100,
		"require": [
			"ts-node/register"
		],
		"extension": [
			".ts"
		],
		"reporter": [
			"lcov",
			"html",
			"text",
			"text-summary"
		],
		"sourceMap": true,
		"instrument": true
	}
}
