{
	"name": "@financialforcedev/orizuru-transport-rabbitmq",
	"version": "3.2.1",
	"description": "Rabbitmq transport layer for the orizuru package",
	"main": "src/lib/index.js",
	"scripts": {
		"doc": "jsdoc src/lib -r -d doc  --readme readme.md",
		"pretest": "eslint src",
		"single-test": "nyc --all=false mocha",
		"test": "nyc --exclude src/example mocha --recursive src/spec"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/financialforcedev/orizuru-transport-rabbitmq"
	},
	"keywords": [
		"rabbitmq",
		"publish",
		"subscribe",
		"heroku",
		"worker",
		"dyno",
		"orizuru"
	],
	"author": "financialforce",
	"license": "BSD-3-Clause",
	"dependencies": {
		"amqplib": "0.5.2",
		"lodash": "4.17.10"
	},
	"devDependencies": {
		"@financialforcedev/eslint-config": "4.0.0",
		"app-root-path": "2.0.1",
		"chai": "4.1.2",
		"chai-as-promised": "7.1.1",
		"jsdoc": "3.5.5",
		"mocha": "5.2.0",
		"nyc": "11.8.0",
		"proxyquire": "2.0.1",
		"sinon": "5.0.10",
		"sinon-chai": "3.1.0",
		"typescript": "2.8.3"
	},
	"nyc": {
		"check-coverage": true,
		"per-file": true,
		"lines": 100,
		"statements": 100,
		"functions": 100,
		"branches": 100,
		"include": [
			"src"
		],
		"reporter": [
			"lcov",
			"html",
			"text",
			"text-summary"
		],
		"cache": true,
		"all": true
	}
}
